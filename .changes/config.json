{
  "gitSiteUrl": "https://github.com/tauri-apps/tauri-action/",
  "pkgManagers": {
    "javascript": {
      "version": true,
      "build": false,
      "preversion": "yarn pkg",
      "publish": false,
      "getPublishedVersion": false,
      "assets": false
    }
  },
  "packages": {
    "core": {
      "path": "./packages/core",
      "manager": "javascript"
    },
    "action": {
      "path": "./packages/action",
      "manager": "javascript",
      "dependencies": [
        "core"
      ]
    }
  }
}
